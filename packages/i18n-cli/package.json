{
  "name": "@gravity-ui/i18n-cli",
  "version": "1.1.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/gravity-ui/i18n.git"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./config": {
      "types": "./dist/config/index.d.ts",
      "import": "./dist/config/index.mjs",
      "require": "./dist/config/index.js",
      "default": "./dist/config/index.js"
    },
    "./dist/*": {
      "types": "./dist/*.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "config": [
        "dist/config/index.d.ts"
      ]
    }
  },
  "files": [
    "bin",
    "dist"
  ],
  "bin": {
    "i18n-cli": "./bin/i18n-cli.js"
  },
  "scripts": {
    "build": "pnpm run build:clean && pnpm run build:tsup && pnpm run build:bin",
    "build:tsup": "tsup",
    "build:bin": "chmod +x bin/i18n-cli.js",
    "build:clean": "rm -rf dist",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "jest",
    "typecheck": "tsc --noEmit"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "i18n"
  ],
  "dependencies": {
    "@gravity-ui/i18n-types": "workspace:*",
    "@typescript-eslint/parser": "8.14.1-alpha.6",
    "@typescript-eslint/typescript-estree": "^8.26.0",
    "astring": "^1.9.0",
    "cosmiconfig": "^9.0.0",
    "cosmiconfig-typescript-loader": "^6.1.0",
    "estree-toolkit": "^1.7.8",
    "format-message-parse": "^6.2.4",
    "format-message-print": "^6.2.4",
    "picocolors": "^1.1.1",
    "yargs": "^16.2.0"
  },
  "devDependencies": {
    "@types/estree": "^1.0.8",
    "@types/estree-jsx": "^1.0.5",
    "@types/jest": "catalog:",
    "@types/node": "^24.7.0",
    "@types/yargs": "^17.0.33",
    "@typescript-eslint/types": "^8.25.0",
    "@typescript-eslint/utils": "^8.25.0",
    "eslint": "catalog:",
    "eslint-config": "workspace:*",
    "jest": "catalog:",
    "prettier": "catalog:",
    "ts-jest": "catalog:",
    "tslib": "catalog:",
    "tsup": "^8.0.1",
    "typescript": "catalog:"
  },
  "peerDependencies": {
    "prettier": "^3.4.2"
  },
  "peerDependenciesMeta": {
    "prettier": {
      "optional": true
    }
  },
  "engines": {
    "node": ">= 18"
  }
}
