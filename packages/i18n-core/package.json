{
  "name": "@gravity-ui/i18n-core",
  "version": "1.4.2",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/gravity-ui/i18n.git"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm run build:clean && pnpm run build:compile",
    "build:compile": "tsc -p tsconfig.json && tsc -p tsconfig.cjs.json",
    "build:clean": "rm -rf dist",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "jest",
    "typecheck": "tsc --noEmit"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "i18n"
  ],
  "dependencies": {
    "@formatjs/fast-memoize": "^2.2.7",
    "@formatjs/intl": "catalog:",
    "intl-messageformat": "catalog:",
    "@gravity-ui/i18n-types": "workspace:*"
  },
  "devDependencies": {
    "eslint-config": "workspace:*",
    "@types/jest": "catalog:",
    "jest": "catalog:",
    "ts-jest": "catalog:",
    "typescript": "catalog:",
    "eslint": "catalog:"
  },
  "engines": {
    "node": ">= 18"
  }
}
