{
  "name": "@gravity-ui/i18n",
  "description": "i18n library for Yandex Cloud UI services",
  "version": "0.6.0",
  "license": "MIT",
  "main": "build/cjs/index.js",
  "module": "build/esm/index.js",
  "types": "build/esm/index.d.ts",
  "repository": "git@github.com:gravity-ui/i18n",
  "scripts": {
    "build": "npm run build:clean && npm run build:compile",
    "build:compile": "tsc -p tsconfig.json && tsc -p tsconfig.cjs.json",
    "build:clean": "rm -rf build",
    "lint": "eslint src/*",
    "prepublish": "npm run build",
    "test": "jest",
    "typecheck": "tsc --noEmit"
  },
  "files": [
    "example",
    "build"
  ],
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@commitlint/cli": "15.0.0",
    "@commitlint/config-conventional": "15.0.0",
    "@gravity-ui/eslint-config": "^1.0.0",
    "@gravity-ui/tsconfig": "^1.0.0",
    "@types/jest": "27.4.1",
    "eslint": "8.3.0",
    "jest": "27.5.1",
    "ts-jest": "27.1.3",
    "typescript": "4.5.4"
  }
}
